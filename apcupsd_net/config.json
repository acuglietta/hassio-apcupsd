{
    "name": "APC UPS Daemon (No USB)",
    "version": "1.5",
    "slug": "apcupsd_net",
    "description": "View information from APC Network UPSs",
    "url": "https://github.com/korylprince/hassio-apcupsd/tree/master/apcupsd_net",
    "image": "korylprince/hassio-apcupsd_net-{arch}",
    "startup": "services",
    "boot": "auto",
    "hassio_api": true,
    "map": ["share"],
    "options": {
        "name": "APC UPS",
        "cable": "ether",
        "type": "net",
        "device": "",
        "extra": []
    },
    "schema": {
        "name": "str",
        "cable": "str",
        "type": "str",
        "device": "str",
        "extra": [
            {"key": "str", "val": "str"}
        ]
    },
    "ports": {
        "3551/tcp": 3551
    }
}
